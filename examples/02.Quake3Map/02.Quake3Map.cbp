<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="02.Quake3Map" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Win32 - Release">
				<Option output="../../bin/Win32/Quake3Map" prefix_auto="1" extension_auto="1" />
				<Option object_output=".obj/" />
				<Option type="1" />
				<Option compiler="ldc" />
				<Compiler>
					<Add option="-O" />
					<Add option="-D" />
					<Add option="-Dd=../../doc" />
					<Add directory="../../src" />
				</Compiler>
				<Linker>
					<Add library="DIrrlicht" />
					<Add library="CIrrlicht" />
					<Add library="Irrlicht" />
					<Add directory="../../lib/Win32-gdc" />
					<Add directory="$(IRRLICHT_ROOT)/lib/Win32-gcc" />
					<Add directory="$(CIRRLICHT_ROOT)/lib/Win32-gcc" />
				</Linker>
			</Target>
			<Target title="Linux - LDC_Release">
				<Option output="../../bin/Linux/Quake3Map" prefix_auto="1" extension_auto="1" />
				<Option object_output=".obj/" />
				<Option type="1" />
				<Option compiler="ldc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="2" />
				<Compiler>
					<Add option="-O" />
					<Add directory="../../src" />
				</Compiler>
				<Linker>
					<Add library="DIrrlicht" />
					<Add library="CIrrlicht" />
					<Add library="Irrlicht" />
					<Add library="stdc++" />
					<Add library="GL" />
					<Add library="Xxf86vm" />
					<Add library="X11" />
					<Add directory="../../lib/Linux" />
					<Add directory="$(HOME)/irrlicht/lib/Linux" />
					<Add directory="$(HOME)/CIrrlicht/lib/Linux" />
				</Linker>
			</Target>
			<Target title="Linux - GDC_Release">
				<Option output="../../bin/Linux/Quake3Map" prefix_auto="1" extension_auto="1" />
				<Option object_output=".obj/" />
				<Option type="1" />
				<Option compiler="gdc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="2" />
				<Compiler>
					<Add option="-O" />
					<Add option="-O" />
					<Add directory="../../src" />
				</Compiler>
				<Linker>
					<Add library="DIrrlicht" />
					<Add library="CIrrlicht" />
					<Add library="Irrlicht" />
					<Add library="stdc++" />
					<Add library="GL" />
					<Add library="Xxf86vm" />
					<Add library="X11" />
					<Add directory="../../lib/Linux" />
					<Add directory="$(HOME)/irrlicht/lib/Linux" />
					<Add directory="$(HOME)/CIrrlicht/lib/Linux" />
				</Linker>
			</Target>
			<Target title="Linux - DMD_Release">
				<Option output="../../bin/Linux/Quake3Map" prefix_auto="1" extension_auto="1" />
				<Option object_output=".obj/" />
				<Option type="1" />
				<Option compiler="dmd" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="2" />
				<Compiler>
					<Add directory="../../src" />
				</Compiler>
				<Linker>
					<Add library="DIrrlicht" />
					<Add library="CIrrlicht" />
					<Add library="Irrlicht" />
					<Add library="stdc++" />
					<Add library="GL" />
					<Add library="Xxf86vm" />
					<Add library="X11" />
					<Add directory="../../lib/Linux" />
					<Add directory="$(HOME)/irrlicht/lib/Linux" />
					<Add directory="$(HOME)/CIrrlicht/lib/Linux" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="main.d" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
